{
  "name": "cake-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:pull": "dotenv -e .env.local -- npx prisma db pull",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:generate": "dotenv -e .env.local -- npx prisma generate",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@headlessui/react": "^2.2.2",
    "@prisma/client": "^6.13.0",
    "@sendgrid/mail": "^8.1.5",
    "@stripe/stripe-js": "^7.8.0",
    "bcryptjs": "^3.0.2",
    "messagebird": "^4.0.1",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.2",
    "ovh": "^2.0.3",
    "prisma": "^6.13.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "stripe": "^18.4.0",
    "twilio": "^5.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/stripe": "^8.0.416",
    "autoprefixer": "^10.4.14",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
